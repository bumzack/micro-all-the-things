{
  "openapi": "3.0.1",
  "info": {
    "title": "CustomerPrice Service",
    "description": "CustomerPrice Service",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8981",
      "description": "CustomerPrice Service"
    }
  ],
  "paths": {
    "/api/v1/customerprices/{customerId}": {
      "post": {
        "operationId": "customerPrices",
        "parameters": [
          {
            "name": "customerId",
            "in": "path",
            "description": "Customer ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CustomerPriceEntry"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CustomerPriceEntry": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "customerId": {
            "type": "integer",
            "format": "int64"
          },
          "discount": {
            "type": "number",
            "format": "double"
          },
          "startYear": {
            "type": "integer",
            "format": "int32"
          },
          "endYear": {
            "type": "integer",
            "format": "int32"
          },
          "created": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    }
  }
}
