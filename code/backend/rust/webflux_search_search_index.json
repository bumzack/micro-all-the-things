{
  "openapi": "3.0.1",
  "info": {
    "title": "Search Index Index Service",
    "description": "Search Index Index Service",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:8320",
      "description": "Search Index Index Service"
    }
  ],
  "paths": {
    "/solr/v1/solr/searchindex/search": {
      "post": {
        "operationId": "searchArticle",
        "requestBody": {
          "content": {
            "*/*": {
              "schema": {
                "$ref": "#/components/schemas/SearchMovieIndexRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/MovieSearchResult"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SearchMovieIndexRequest": {
        "type": "object",
        "properties": {
          "q": {
            "type": "string"
          },
          "offset": {
            "type": "integer",
            "format": "int32"
          },
          "limit": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "MovieSearchResult": {
        "type": "object",
        "properties": {
          "movies": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SearchDoc"
            }
          }
        }
      },
      "SearchDoc": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "tconst": {
            "type": "string"
          },
          "titles": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "actors": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "directors": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "genres": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "characters": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "runtimeMinutes": {
            "type": "integer",
            "format": "int32"
          },
          "year": {
            "type": "integer",
            "format": "int32"
          },
          "adult": {
            "type": "boolean"
          },
          "titleType": {
            "type": "string"
          }
        }
      }
    }
  }
}
